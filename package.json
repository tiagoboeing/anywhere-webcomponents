{
  "name": "anywhere-webcomponents",
  "version": "0.0.1",
  "homepage": "https://github.com/tiagoboeing/anywhere-webcomponents#readme",
  "description": "A UI based on web components to use in anywhere",
  "main": "dist/index.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/anywhere-webcomponents/anywhere-webcomponents.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "changelog": "auto-changelog",
    "changelog.debug": "auto-changelog --template json --output changelog-data.json "
  },
  "dependencies": {
    "@stencil/core": "^1.17.3",
    "@types/jest": "^26.0.10"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiagoboeing/anywhere-webcomponents/issues"
  },
  "repository": {
    "url": "https://github.com/tiagoboeing/anywhere-webcomponents"
  },
  "devDependencies": {
    "@stencil/sass": "^1.3.2",
    "@types/puppeteer": "2.0.1",
    "auto-changelog": "^2.0.0",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.0.0",
    "puppeteer-core": "^5.2.1"
  }
}
